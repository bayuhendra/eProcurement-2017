<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field">
    <class mutable="true" name="com.agit.eProcure.core.domain.vendor.DataLogin" table="AGIT_DATALOGIN">
        <id column="id_privilege" name="id">
            <generator class="increment"/>
        </id>
        <property column="idDataLogin" name="idDataLogin" not-null="false"/>
        <property column="namaPengguna" name="namaPengguna"/>
        <property column="idUser" name="idUser"/>
        <property column="password" name="password"/>
        <property column="logo" name="logo"/>
        <property column="headerImage" name="headerImage"/>
        <property column="createdBy" name="createdBy"/>
        <property column="createdDate" name="createdDate"/>
        <property column="modifiedBy" name="modifiedBy"/>
        <property column="modifiedDate" name="modifiedDate"/>
    </class>
</hibernate-mapping>
