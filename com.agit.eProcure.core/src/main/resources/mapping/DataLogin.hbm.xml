<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field">
    <class mutable="true" name="com.agit.eProcure.core.domain.vendor.DataLogin" table="AGIT_DATALOGIN">
        <id column="id_privilege" name="id">
            <generator class="increment"/>
        </id>
        <property column="idDataLogin" name="idDataLogin" unique="true" update="false" not-null="true"/>
        <property column="namaPengguna" name="namaPengguna" not-null="true" update="true"/>
        <property column="idUser" name="idUser"/>
        <property column="password" name="password"/>
        <property column="logo" name="logo"/>
        <property column="headerImage" name="headerImage" unique="true"/>
        <property column="createdBy" name="createdBy" not-null="false"/>
        <property column="createdDate" name="createdDate" not-null="false"/>
        <property column="modifiedBy" name="modifiedBy" not-null="false"/>
        <property column="modifiedDate" name="modifiedDate" not-null="false"/>
    </class>
</hibernate-mapping>
