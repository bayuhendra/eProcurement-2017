<?xml version="1.0" encoding="UTF-8"?>

<zk xmlns="http://www.zkoss.org/2005/zul">
    <window apply="org.zkoss.bind.BindComposer"
            viewModel="@id('vm') @init('com.agit.eProcure.main.viewmodel.vendor.VendorVM')">
        <groupbox width="600px" closable="false">
            <caption label="Tambah Data PIC" style="font-size:16pt; font-weight: bold"/>
            <grid style="border:0">
                <columns>
                    <column width="50%" />
                    <column width="50%" />
                </columns>
                <rows>
                    <row>
                      <label value="Nama" />
                      <textbox />
                    </row>
                    
                    <row>
                      <label value="Jabatan" />
                      <textbox />
                    </row>
                    
                    <row>
                      <label value="Email" />
                      <textbox />
                    </row>
                </rows>
            </grid>
            <grid style="border:0">
                <columns>
                    <column width="50%"/>
                    <column width="15%"/>
                    <column />
                </columns>
                <rows>
                    <row>
                        <label/>
                        <button label="Kembali" onClick="@command('buttonKlikBackDataPerusahaanForm')" width="100%"/>
                        <button label="Save" onClick="@command('buttonKlikSaveDataPerusahaanForm')" width="40%"/>
                    </row>
                </rows>
            </grid>
        </groupbox>
    </window>
</zk>
