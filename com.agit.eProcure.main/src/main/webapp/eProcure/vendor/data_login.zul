<zk xmlns:x="xhtml">
    <window>
        <separator spacing="5px"/>
        <groupbox width="600px">
            <caption label="Data Login" style="font-size:16pt"/>
            <grid style="border:0">
                <columns>
                    <column width="35%"></column>
                    <column ></column>
                </columns>
                <rows>
                    <row>
                        <label value="Nama Pengguna " />
                        <textbox width="100%" disabled="false" constraint="no empty : ID Ticket Tidak Boleh Kosong"/>
                    </row>
                    <row>
                        <label value="User ID"/>
                        <textbox width="100%" disabled="false" constraint="no empty : ID Ticket Tidak Boleh Kosong"/>
                        <button label="Check User ID" width="60%"></button>
                    </row>
                    <row>
                        <label value="Password"/>
                        <textbox width="100%" disabled="false" constraint="no empty : ID Ticket Tidak Boleh Kosong"/>
                    </row>
                    <row>
                        <label value="Retype Password"/>
                        <textbox width="100%" disabled="false" constraint="no empty : ID Ticket Tidak Boleh Kosong"/>
                    </row>
                    <row>
                        <label value="Logo"/>
                        <textbox width="100%" readonly="true"/>
                        <button label="Upload" upload="true" onUpload="@command('uploadFile')" width="60%">
                        </button>
                    </row>
                    <row>
                        <label value="Header Image"/>
                        <textbox width="100%" readonly="true"/>
                        <button label="Upload" upload="true" onUpload="@command('uploadFile')" width="60%">
                        </button>
                    </row>
                </rows>
            </grid>
        </groupbox>
    </window>
</zk>