<zk xmlns:x="xhtml">
    <window apply="org.zkoss.bind.BindComposer"
            viewModel="@id('vm') @init('com.agit.eProcure.main.viewmodel.vendor.VendorVM')">
        <separator spacing="5px"/>
        <groupbox width="600px">
            <caption label="Data Login" style="font-size:16pt;font-weight: bold"/>
            <grid style="border:0">
                <columns>
                    <column width="3%"></column>
                    <column width="20%"></column>
                    <column ></column>
                </columns>
                <rows>
                    <row>
                        <label value="*) " style="color:red"/>
                        <label value="Nama Pengguna " style="font-weight: bold"/>
                        <textbox width="100%" disabled="false" constraint="no empty : ID Ticket Tidak Boleh Kosong"/>
                    </row>
                    <row>
                        <label value="*) " style="color:red"/>
                        <label value="User ID" style="font-weight: bold"/>
                        <textbox width="100%" disabled="false" constraint="no empty : ID Ticket Tidak Boleh Kosong"/>
                        <button label="Check User ID" width="60%"></button>
                    </row>
                    <row>
                        <label value="*) " style="color:red"/>
                        <label value="Password" style="font-weight: bold"/>
                        <textbox width="100%" disabled="false" type="password"/>
                    </row>
                    <row>
                        <label value="*) " style="color:red"/>
                        <label value="Retype Password" style="font-weight: bold"/>
                        <textbox width="100%" disabled="false" type="password"/>
                    </row>
                    <row>
                        <label value="" style="color:red"/>
                        <label value="  Logo" style="font-weight: bold"/>
                        <textbox width="100%" readonly="true"/>
                        <button label="  Upload" upload="true" onUpload="@command('uploadFile')" width="60%">
                        </button>
                    </row>
                    <row>
                        <label value="" style="color:red"/>
                        <label value="Header Image" style="font-weight: bold"/>
                        <textbox width="100%" readonly="true"/>
                        <button label="Upload" upload="true" onUpload="@command('uploadFile')" width="60%">
                        </button>
                    </row>
                </rows>
            </grid>
            
            <groupbox>
                <label value="*) " style="color:red"/>
                <label value="User ID dan Password akan di gunakan saat login"/>
                <separator></separator>
                <label value="*) " style="color:red"/>
                <label value="Password minimal 8 karakter, kombinasi huruf besar, kecil dan angka"/>
                <separator></separator>
                <label value="*) " style="color:red"/>
                <label value="Dengan mengisi formulir registrasi ini anda telah menyetujui TERM CONDITION E-PROCUREMENT"/>
                <separator></separator>
                
            </groupbox>
            <grid style="border:0">
                
                <columns>
                    <column align="right" width="50%"/>
                    <column align="left" width="50%"/>
                </columns>
                
                <rows>
                    <row>
                        <button label="Kembali" width="30%" />
                        <button label="Selanjutnya" width="30%" />
                    </row>
                </rows>
            </grid>
        </groupbox>
        
    </window>
</zk>